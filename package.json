{
  "name": "cap-training",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "express": "^4",
    "@sap/xssec": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2.0.4",
    "@sap/cds-dk": "^9"
  },
  "cds": {
    "requires": {
      "db": {
        "[development]": {
          "kind": "sqlite",
          "credentials": {
            "url": ":memory:"
          }
        }
      },
      "auth": {
        "kind": "mocked",
        "users": {
          "adminuser": {
            "password": "abcd1234",
            "roles": [
              "admin"
            ]
          },
          "catuser": {
            "password": "abcd1234"
          }
        }
      },
      "[production]": {
        "db": {
          "kind": "hana"
        },
        "auth": {
          "kind": "xsuaa"
        }
      }
    }
  },
  "scripts": {
    "start": "cds-serve"
  },
  "private": true
}
